package com.session1;
class A{
	 int i=34;
	public A() {
		System.out.println("ctr of class A");
	}
}
class B extends A{
	 int j=78;
	
	public B() {
		super();
		System.out.println("ctr of class B");
	}
}
public class E_SimpleInheritnceEx {
	
	public static void main(String[] args) {
		
		B b=new B();
		System.out.println(b.i);
		System.out.println(b.j);
		
		
	}

}
